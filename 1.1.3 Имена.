public class Human {
    public String name;
    public String surname;
    public String secondName;

    @Override
    public String toString() {
        String res = "";
        if (name!=null) return res + (name + " ");
        if (surname!=null) return res + (surname + " ");
        if (secondName!=null) return res + (secondName + " ");
        return res;
    }

    public String person(){
        if (secondName==null) return name + " " + surname;
        else if (surname != null && name != null) return surname + " " + name +" " + secondName;
        else if (name==null && surname==null) return secondName;
        return name;
    }
}


public class Main1 {

    public static void main(String[] args) {

        Human human1 = new Human();
        Human human2 = new Human();
        Human human3 = new Human();
        human1.name = "Alina";
        human1.surname = "Kosh";
        human1.secondName = "Den";
        human2.name = "Mary";
        human2.surname = "Frisky";
        human3.secondName = "Renatov";

        System.out.println(human1.person());
        System.out.println(human2.person());
        System.out.println(human3.person());
    }
}
