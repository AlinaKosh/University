public class Name {
    public String name;
    public String surname;
    public String secondName;

    @Override
    public String toString() {
        String res = "";
        if (name!=null)  res += (name + " ");
        if (surname!=null) res += (surname + " ");
        if (secondName!=null) res += (secondName + " ");
        return res;
    }
    
}



public class Main1 {

    public static void main(String[] args) {

        Name name1 = new Human();
        Name name2 = new Human();
        Name name3 = new Human();
        name1.name = "Alina";
        name1.surname = "Kosh";
        name1.secondName = "Den";
        name2.name = "Mary";
        name2.surname = "Frisky";
        name3.secondName = "Renatov";

        System.out.println(name1);
        System.out.println(name2);
        System.out.println(name3);
    }
}
